{
  "name": "mingo",
  "version": "4.1.2",
  "description": "MongoDB query language for in-memory objects",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "docs": "typedoc && touch docs/.nojekyll",
    "build": "tsc && node ./build.js",
    "lint": "eslint --fix --ext .js,.ts src/",
    "test": "npm run build && NODE_ENV=test nyc --reporter=lcov --reporter=text ./runtest.sh && npm run lint",
    "dist": "npm run test && cd lib && npm publish",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "prepublishOnly": "echo \"use 'npm run dist'\" && false",
    "postinstall": "node ./build.js install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kofrasa/mingo.git"
  },
  "files": [
    "lib/",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "codecov": "3.8.1",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unused-imports": "1.1.0",
    "esm": "3.2.25",
    "lodash": "4.17.21",
    "nyc": "15.1.0",
    "perf_hooks": "0.0.1",
    "prettier": "2.2.1",
    "tape": "5.2.1",
    "typedoc": "0.20.28",
    "typescript": "4.2.2"
  },
  "keywords": [
    "util",
    "mongo",
    "nosql",
    "query",
    "aggregate",
    "filter",
    "group",
    "project",
    "search",
    "transform"
  ],
  "author": {
    "name": "Francis Asante",
    "email": "kofrasa@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kofrasa/mingo/issues"
  }
}
